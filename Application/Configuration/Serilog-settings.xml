<?xml version="1.0" encoding="utf-8"?>
<LogConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <WriteToConsole>true</WriteToConsole>
  <LogEventLevel>Debug</LogEventLevel>
  <LogFormat>{Timestamp:HH:mm:ss} {FileUrl,36}:{LineNumber,-4} {ThreadName,-16} {Level,-12} {Message:lj}{NewLine}</LogFormat>
  <Theme>Sixteen</Theme>
</LogConfiguration>